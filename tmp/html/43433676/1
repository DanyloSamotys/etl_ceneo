        require(['jquery', 'modernizr.min', 'Desktop/base/custom/stickyHeader', 'Shared/utils/utils'], function ($, modernizr, stickyHeader) {
            
                if (Modernizr.touch) {
                    return;
                }

                var sHeader = stickyHeader.getInstance();

                sHeader.init(true);

                $('.js_toggle-sticky-header').on('click', function (e) {
                    var btn = $('.js_toggle-sticky-header');

                    if (sHeader.isEnabled()) {
                        sHeader.disable();
                        btn.html($txt('global_enable'));
                    } else {
                        sHeader.enable();
                        btn.html($txt('global_disable'));
                    }
                });
        });

    </script><script>
