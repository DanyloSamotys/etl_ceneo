            <a href="#reportTechSpecsBug" class="dotted-link js_addProductProblemLink">Zgłoś błąd w specyfikacji</a>
<script id="reportProductProblem" type="text/html">
    <form action="#" name="productProblemForm">
        <p>Opisz problem lub zaproponuj zmiany <abbr class="required" title="Pole wymagane">*</abbr></p>
        <div class="row">
            <input type="hidden" name="ProductId" value="43433676" />
            <input type="hidden" name="Type" value="Issue" />
            <textarea class="required" cols="100" data-val="true" data-val-required="The Message field is required." id="Message" name="Message" rows="5">
</textarea>            
        </div>
        <div class="buttons">
            <input class="btn btn-primary" type="submit" value="Wyślij" />
        </div>
    </form>
</script>

<script>
    require(['jquery', 'Shared/utils/utils', 'Shared/Popup', 'Shared/utils/preloader'], function ($, utils, popup, preloader) {
        $(document).ready(function () {
            var template = $($('#reportProductProblem').html()),
                productProblemPopup = new popup().init();

            productProblemPopup.getContent().on('submit', 'form', function (e) {
                e.preventDefault();

                var form = $(this);

                form.validate(
                    $.extend({
                        messages: {
                            Message: 'Pole wymagane'
                        }
                    }, utils.errorMsgFormat())
                    );

                if (form.valid()) {
                    productProblemPopup.updateContent(preloader.render());
                    $.post(
                        '/addProductProblem',
                        form.serialize(),
                        function (data) {
                            productProblemPopup.updateContent(
                                '<p class="msg">' + data + '</p>' +
                                '<div class="buttons"><button class="btn btn-primary" data-close="">Ok</button></div>'
                                );
                        }
                        );
                }
            });

            $('.js_addProductProblemLink').click(function (e) {
                e.preventDefault();
                productProblemPopup.runStatic(template);
            });
        });
    });
    </script>                <script>
            require(['jquery', 'Shared/basket/contextHelp'], function ($, contextHelp) {
                contextHelp.init();
                $('.js_productTechSpecsSyncTip').contextHelp();
                $('.js_productTechSpecsAsyncTip').contextHelp(function (element, renderFunc) {
                    $.ajax({
                        url: $(element).attr('data-asynctipurl'),
                        type: 'GET',
                        success: function (data) {
                            renderFunc(data);
                        }
                    });
                });
            });
        </script>
    </footer>

