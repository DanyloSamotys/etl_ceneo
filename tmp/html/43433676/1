    <script type='text/javascript'>
        googletag.cmd.push(function () {

            function getTag() {
                var validCharsExpr = '[\\w\\-$.+!*\'()]+?';

                function getHashFragmentTag(){
                    var pattern = '^#({c}={c}&){0,}tag=({c})(&{c}={c}){0,}$';
                    pattern = pattern.split('{c}').join(validCharsExpr);

                    var matches = document.location.hash.match(pattern);

                    if (matches) {
                        return matches[2];
                    } else {
                        return "";
                    }
                }

                function getQueryStringTag(){
                    var pattern = '^[^?]+\\?({c}={c}&)*tag=({c})(&{c}={c})*(#[\\w\\-$.+!*\'()%=&]*)?$';
                    pattern = pattern.split('{c}').join(validCharsExpr);

                    var matches = document.location.href.match(pattern);

                    if (matches) {
                        return matches[2];
                    } else {
                        return "";
                    }
                }

                var hashFragmentTag = getHashFragmentTag();
                if (hashFragmentTag) {
                    return hashFragmentTag;
                } else {
                    return getQueryStringTag();
                }
            }

