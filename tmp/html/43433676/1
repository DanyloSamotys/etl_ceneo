        require(['jquery', 'Shared/universalAnalyticsHelper'], function ($, universalAnalyticsHelper) {
            $('.installments-anchor').on('click', function (e) {
                var action = $(e.target).data('google-action');
                var label = $(e.target).data('google-label');
                var gaCategory = 'Installments';

                _gaq.push(['_trackEvent', gaCategory, action, label, 0, true]);

                try {
                    universalAnalyticsHelper.sendEvent(gaCategory, action, label);
                } catch (e) {
                }
            });
        });
        
