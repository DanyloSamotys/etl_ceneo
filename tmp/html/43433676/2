    <script>
                require(['jquery', 'domReady!'],
                    function ($) {
                        var btn = $('#js_cookie-monster-dismiss'),
                            msg = $('#js_cookie-monster');

                        btn.css('display', 'inline-block');
                        btn.click(function (e) {
                            e.preventDefault();
                            msg.remove();

                            var date = new Date();
                            date.setDate(date.getDate() + 99999);

                            document.cookie = "cnicaf=; expires=" + date.toUTCString() + "; path=/";
                        });
